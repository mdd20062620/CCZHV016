<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl_order_item"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:ignore="MissingPrefix">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical" >
            <TextView
                android:id="@+id/dateSave"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/deviceNumSave"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/companySave"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/chacheNumber"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/chacheType"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/chacheGroup"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/breakSpeed"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_marginTop="10dp"
                android:layout_weight="1"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/ratedSpeed"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/breakDis"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:textColor="@color/white"
                android:text=""
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/breakTime"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/breakForce"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/breaklength"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/angleErr"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:text=""
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
            <TextView
                android:id="@+id/forceErr"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:layout_marginTop="10dp"
                android:gravity="left|center"
                android:textColor="@color/white"
                android:textSize="20sp"
                fontPath="fonts/楷体.ttf"/>
        </LinearLayout>
</LinearLayout>
